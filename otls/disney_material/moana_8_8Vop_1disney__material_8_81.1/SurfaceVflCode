    assign(output1,output2,output3, scatterDistance);


    vector	_basecolor = out;
    int	_basecolor_usePointColor = 0;
    float	_ior = ior;
    float	_rough = roughness;
    float	_aniso = anisotropic;
    float	_metallic = metallic;
    float	_reflect = specTrans;
    float	_reflecttint = specularTint;
    float	_coat = clearcoat;
    float	_coatrough = shift;
    float	_transparency = result5;
    float	_sssdist = output1;
    float	_sheen = sheen;
    float	_sheentint = sheenTint;
    float	_opac = alpha;
    int	_opacpointalpha = 0;
    ShaderLayer	layer = {};
    vector	_tmp_Cf = Cf;
    vector	_tmp_Of = Of;
    float	_tmp_Af = Af;
    vector	_tmp_N = N;
    bsdf	_tmp_F = bsdf();
    principledshader__2_0("basecolor", _basecolor, "basecolor_usePointColor", _basecolor_usePointColor, "ior", _ior, "rough", _rough, "aniso", _aniso, "metallic", _metallic, "reflect", _reflect, "reflecttint", _reflecttint, "coat", _coat, "coatrough", _coatrough, "transparency", _transparency, "sssdist", _sssdist, "sheen", _sheen, "sheentint", _sheentint, "opac", _opac, "opacpointalpha", _opacpointalpha, "layer", layer, "Cf", _tmp_Cf, "Of", _tmp_Of, "Af", _tmp_Af, "N", _tmp_N, "F", _tmp_F);
    Cf = _tmp_Cf;
    Of = _tmp_Of;
    Af = _tmp_Af;
    N = _tmp_N;
    F = _tmp_F;
}
